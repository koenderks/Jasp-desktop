{
	"name": "MultinomialTest",
	"autorun": true,
	"version": "1.00",
	"options": [
    {
      "name": "factor",
      "type": "Variable"
    },
    {
      "name": "counts",
      "type": "Variable"
    },
    {
      "name": "exProbVar",
      "type": "Variable"
    },
    {
      "name": "hypothesis",
      "options": [ "multinomialTest", "expectedProbs" ],
      "default": "multinomialTest",
      "type": "List"
    },
    {
      "name": "countProp",
      "options": [ "descCounts", "descProps" ],
      "default": "descCounts",
      "type": "List"
    },
		{
			"name": "descriptives",
			"type": "Boolean",
			"default": false
		},
    {
			"name": "confidenceInterval",
			"type": "Boolean"
		},
		{
			"format": "%",
			"max": 0.999,
			"min": 0.001,
			"name": "confidenceIntervalInterval",
			"type": "Number",
			"value": 0.950
		},
		{
			"name": "descriptivesPlot",
			"type": "Boolean"
		},
		{
			"format": "%",
			"max": 0.999,
			"min": 0.001,
			"name": "descriptivesPlotConfidenceInterval",
			"type": "Number",
			"value": 0.950
		},
		{
		  "name": "VovkSellkeMPR",
		  "type": "Boolean",
		  "default": false
		},
		{
			"name": "plotWidth",
			"type": "Integer",
			"default": 450
		},
		{
			"name": "plotHeight",
			"type": "Integer",
			"default": 300
		}
  ]
}
