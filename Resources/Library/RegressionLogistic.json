{
	"name": "RegressionLogistic",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "dependent",
			"type": "Variable"
		},
		{
			"name": "covariates",
			"type": "Variables"
		},
		{
			"name": "factors",
			"type": "Variables"
		},
		{
			"name": "wlsWeights",
			"type": "Variable"
		},
		{
			"name": "modelTerms",
			"template": [
				{
					"name": "components",
					"type": "Term"
				}, 
				{
					"default": false,
					"name": "isNuisance",
					"type": "Boolean"
				}
			],
			"type": "Table"
		},
		{
			"name": "includeIntercept",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "coeffEstimates",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "coeffCI",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "coeffCIInterval",
			"type": "Integer",
			"min": 1,
			"max": 99,
			"default": 95
		},
		{
			"name": "coeffCIOR",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "stdCoeff",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "oddsRatios",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "VovkSellkeMPR",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "robustSEOpt",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "confusionMatrixOpt",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "confusionMatrixProportions",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "AUC",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "Sens",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "Spec",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "Prec",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "Fmsr",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "BrierScr",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "Hmsr",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "estimatesPlotsOpt",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "estimatesPlotsCI",
			"type": "Integer",
			"min": 1,
			"max": 99,
			"default": 95
		},
		{
			"name": "showPoints",
			"type": "Boolean",
			"default": true
		},
		{
			"name": "predictedPlotOpt",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "predictorPlotsOpt",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "squaredPearsonPlotOpt",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "residualType",
			"options": [ "deviance", "pearson" ],
			"default": "deviance",
			"type": "List"
		},
		{
			"name": "factorDescriptivesOpt",
			"type": "Boolean",
			"default": false
		},
		{
			"name": "plotWidth",
			"type": "Integer",
			"default": 480
		},
		{
			"name": "plotHeight",
			"type": "Integer",
			"default": 320
		}
	]
}
