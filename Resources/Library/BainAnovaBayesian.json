{
	"name": "BainAnovaBayesian",
	"autorun": true,
	"version": "1.00",
	"options": [
		{
			"name": "model",
			"type": "String"
		},
		{
			"name": "dependent",
			"type": "Variable"
		},
		{
			"name": "fixedFactors",
			"type": "Variable"
        },
		{
            "name": "descriptives",
			"type": "Boolean"
        },
		{
            "options": [ "BF01", "BF10" ],
            "default": "BF10",
			"name": "bayesFactorType",
			"type": "List"
		},
		{
            "options": ["BF", "logBF"],
            "default": "BF",
            "name": "logScale",
            "type": "List"
        },
        {
            "name": "BFplot",
            "type": "Boolean"
        },
		{
			"name": "BFmatrix",
			"type": "Boolean"
		},
        {
            "name": "CredibleInterval",
            "type": "Integer",
            "default": 95
        },
		{
			"name": "plotWidth",
			"type": "Integer",
			"default": 480
		},
		{
			"name": "plotHeight",
			"type": "Integer",
			"default": 320
		},
		{
			"name": "plotDescriptives",
			"type": "Boolean"
		}
	]
}
